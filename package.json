{
  "name": "quiz.akp.tools",
  "version": "1.0.0",
  "main": "dist/index.html",
  "repository": "https://github.com/akp-tools/quiz.akp.tools",
  "author": "Austin Peterson <austin@akpwebdesign.com>",
  "license": "MIT",
  "browserslist": [
    "> 0.2%",
    "not dead"
  ],
  "scripts": {
    "build": "parcel build public/index.html",
    "clean": "rimraf dist",
    "deepclean": "rimraf dist .parcel-cache",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "powerwash": "rimraf dist .parcel-cache node_modules && yarn",
    "start": "parcel public/index.html",
    "typecheck": "tsc --noEmit"
  },
  "babelMacros": {
    "twin": {
      "styled": {
        "import": "default",
        "from": "@emotion/styled"
      },
      "css": {
        "import": "css",
        "from": "@emotion/react"
      },
      "global": {
        "import": "Global",
        "from": "@emotion/react"
      },
      "config": "tailwind.config.js",
      "debugProp": true,
      "debugPlugins": false,
      "debug": false
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@emotion/babel-plugin-jsx-pragmatic": "^0.1.5",
    "@parcel/validator-typescript": "^2.0.0-alpha.3",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.3.1",
    "@types/node": "12.19.6",
    "@types/parcel-env": "^0.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "autoprefixer": "^10.0.2",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "firebase-tools": "^8.16.2",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "parcel": "^2.0.0-nightly.446",
    "postcss": "^8.1.9",
    "postcss-import": "^13.0.0",
    "postcss-modules": "^3.2.2",
    "prettier": "^2.2.0",
    "rimraf": "^3.0.2",
    "tailwind": "^4.0.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@emotion/react": "^11.1.1",
    "@emotion/styled": "^11.0.0",
    "core-js": "3",
    "firebase": "^7.21.1",
    "react": "^0.0.0-experimental-4ead6b530",
    "react-dom": "^0.0.0-experimental-4ead6b530",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "reactfire": "^2.0.3",
    "regenerator-runtime": "^0.13.7",
    "tailwindcss": "^1.9.0",
    "twin.macro": "^1.12.1"
  }
}
